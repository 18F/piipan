{
    "fields": [
      {
        "name": "last",
        "type": "string",
        "description": "Participant's last name",
        "constraints": {
            "required": true,
            "minLength": 1
        }
      },
      {
        "name": "first",
        "type": "string",
        "description": "Participant's first name",
        "constraints": {
            "required": true,
            "minLength": 1
        }
      },
      {
        "name": "middle",
        "type": "string",
        "description": "Participant's middle name"
      },
      {
        "name": "dob",
        "type": "date",
        "format": "%m/%d/%Y",
        "description": "Participant's date of birth",
        "constraints": {
            "required": true
        }
      },
      {
        "name": "ssn",
        "type": "string",
        "description": "Participant's Social Security Number",
        "constraints": {
            "required": true,
            "minLength": 11,
            "maxLength": 11,
            "unique": true,
            "pattern": "^[0-9]{3}-[0-9]{2}-[0-9]{4}$"
        }
      },
      {
        "name": "exception",
        "type": "string",
        "description": "Placeholder for value indicating special processing instructions"
      },
      {
        "name": "case_id",
        "type": "string",
        "description": "Participant's state-specific case identifier",
        "constraints": {
          "required": true,
          "minLength": 1
        }
      },
      {
        "name": "participant_id",
        "type": "string",
        "description": "Participant's state-specific identifier. Must not be social security number or any personal identifiable information."
      },
      {
        "name": "benefits_end_month",
        "type": "yearmonth",
        "description": "month and year when participant's benefits end"
      }
    ]
}
