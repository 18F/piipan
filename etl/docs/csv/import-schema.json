{
    "fields": [
      {
        "name": "last",
        "type": "string",
        "description": "Participant's last name",
        "constraints": {
            "required": true,
            "minLength": 1
        }
      },
      {
        "name": "first",
        "type": "string",
        "description": "Participant's first name"
      },
      {
        "name": "middle",
        "type": "string",
        "description": "Participant's middle name"
      },
      {
        "name": "dob",
        "type": "date",
        "format": "%m/%d/%Y", 
        "description": "Participant's date of birth",
        "constraints": {
            "required": true
        }
      },
      {
        "name": "ssn",
        "type": "string",
        "description": "Participant's Social Security Number",
        "constraints": {
            "required": true,
            "minLength": 11,
            "maxLength": 11,
            "unique": true,
            "pattern": "^[0-9]{3}-[0-9]{2}-[0-9]{4}$"
        }
      },
      {
        "name": "exception",
        "type": "string",
        "description": "Placeholder for value indicating special processing instructions"
      },
      {
        "name": "case id",
        "type": "string",
        "description": "state-provided relevant case number or identifier",
        "constraints": {
          "required": true
        }
      },
      {
        "name": "participant id",
        "type": "string",
        "description": "state-provided identifier of the participant. Should not be social security number or any personal identifiable information."
      }
    ]
  }
