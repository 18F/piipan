@page
@model IndexModel
@{
    ViewData["Title"] = "NAC Query Tool";
}
<div class="text-center">
    <h1>NAC Query Tool</h1>
    <form action="">
        <div><label for="last">Last Name:</label><input type="text" name="last"></div>
        <div><label for="middle">Middle Name:</label><input type="text" name="middle"></div>
        <div><label for="first">First Name</label><input type="text" name="first"></div>
        <div><label for="ssn">Social Security Number</label><input type="text" name="ssn"></div>
        <div><label for="dob">Date of Birth</label><input type="text" name="dob"></div>
        <button type="submit">Submit</button>
    </form>
</div>
